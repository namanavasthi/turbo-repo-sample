{
  "name": "plugin",
  "version": "0.0.0",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "sideEffects": false,
  "license": "MIT",
  "files": [
    "dist/**"
  ],
  "scripts": {
    "components":  "cat dist/components/*.css > dist/styled.css",
    "prejss": "prejss-cli dist/styled.css --format commonjs",

    "build:plugin": "npm run components && npm run prejss && tsup index.ts --format esm,cjs --dts",

    "dev": "tsup index.ts --format esm,cjs --watch --dts",
    "lint": "TIMING=1 eslint *.ts --fix",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist"
  },
  "devDependencies": {
    "@types/react": "^18.0.9",
    "@types/react-dom": "^18.0.4",
    "eslint": "^8.15.0",
    "eslint-config-acme": "*",
    "prejss-cli": "^0.3.3",
    "react": "^18.1.0",
    "tailwindcss": "^3.1.8",
    "tsconfig": "*",
    "tsup": "^5.10.1",
    "typescript": "^4.5.3"
  },
  "publishConfig": {
    "access": "public"
  }
}
